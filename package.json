{
  "name": "hof-rds-api",
  "version": "1.0.0",
  "description": "A repo for a backend microservice for hof services to communicate with RDS instances",
  "main": "server.js",
  "engines": {
    "node": "^18.12.0"
  },
  "scripts": {
    "start": "node server.js",
    "start:dev": "nodemon -r dotenv/config --ignore data/ server.js",
    "db:setup": "yarn run db:migrate && yarn run db:seed",
    "db:test": "yarn run db:setup && yarn run db:rollback",
    "db:migrate": "node -e 'require(\"./db\").migrate()'",
    "db:local:migrate": "node -r dotenv/config -e 'require(\"./db\").migrate()'",
    "db:local:delete": "node -r dotenv/config -e 'require(\"./db\").deleteOldTableData(`saved_applications`)'",
    "db:rollback": "node -e 'require(\"./db\").rollback()'",
    "db:seed": "knex seed:run",
    "dev:db:migrate": "node -r dotenv/config -e 'require(\"./db\").migrate()' dotenv_config_path=.env-prod",
    "test:lint": "eslint . --config ./node_modules/eslint-config-hof/default.js",
    "test:snyk": "snyk config set api=SNYK_TOKEN && snyk test"
  },
  "author": "Alex Swann",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.4.0",
    "body-parser": "^1.20.2",
    "express": "^4.18.2",
    "jquery": "^3.6.4",
    "knex": "^0.95.11",
    "knex-migrate": "^1.7.4",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "morgan-json": "^1.1.0",
    "node-cron": "^3.0.2",
    "pg": "^8.7.1",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "dotenv": "^16.0.3",
    "eslint": "^8.39.0",
    "eslint-config-hof": "^1.3.1",
    "nodemon": "^2.0.22",
    "snyk": "^1.1143.0"
  }
}
